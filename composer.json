{
  "name": "motomedialab/compliance",
  "description": "A package for enforcing GDPR compliance rules",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "motomedialab",
    "compliance",
    "gdpr",
    "consent"
  ],
  "authors": [
    {
      "name": "Chris Page",
      "email": "chris@motocom.co.uk"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "pestphp/pest": "^2.0|^3.0",
    "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
    "laravel/pint": "^1.18",
    "larastan/larastan": "^2.0|^3.0"
  },
  "autoload": {
    "psr-4": {
      "Motomedialab\\Compliance\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Motomedialab\\Compliance\\": "src/",
      "Motomedialab\\Compliance\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Motomedialab\\Compliance\\Providers\\ComplianceServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
